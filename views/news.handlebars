<!DOCTYPE html>
<html lang="en">
<a href="..">Main</a>
<link rel="stylesheet" href="{{ staticBasePath }}index.css" media="screen">
<header class="header">{{ category }}</header>
<div class="weather">{{> weather}}</div>
<table class='table___news'>
    {{#each articles}}
        <tr>
            <td>{{title}}</td>
            <td></td>
            <td></td>
            <td>{{sformatTime publishedAt "D MMMM"}}</td>
        </tr>
        <tr>
            <td><img src="{{urlToImage}}"></td>
            <td>{{description}}</td>
        </tr>
    {{/each}}
</table>
<footer class="footer">
    <div class="footer__copyright">{{ copyright }}</div>
    <div class="footer__year">{{ year }}</div>
</footer>
</html>
